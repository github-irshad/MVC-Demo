@model List<ASPNETMVCCRUD.Models.Domain.Employee>

  <h1>Employees</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Department</th>
        <th>Salary</th>
        <th>Date of birth</th>
      </tr>
    </thead>

    <tbody>
      @foreach (var employee in Model)
      {
        <tr>
          <td>@employee.Id</td>
          <td>@employee.Name</td>
          <td>@employee.Email</td>
          <td>@employee.Department</td>
          <td>@employee.Salary<td>
          <td>@employee.DateOfBirth.ToString("dd-MM-yyyy")</td>
        </tr>
      }
    </tbody>
  </table>